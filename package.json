{
    "name": "neos-fusion",
    "displayName": "neos-fusion",
    "description": "Fusion and Eel language support for Neos CMS",
    "version": "1.0.0",
    "publisher": "networkteam",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "fusion",
                "aliases": [
                    "Fusion",
                    "fusion"
                ],
                "extensions": [
                    ".fusion"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fusion",
                "scopeName": "source.fusion",
                "path": "./syntaxes/fusion.tmLanguage.json"
            }
        ]
    }
}